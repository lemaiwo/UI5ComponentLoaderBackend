"use strict";                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
sap.ui.define(["be/wl/zcomploaderlib/objects/BaseService", "sap/ui/model/Sorter", "sap/ui/model/Filter", "sap/ui/model/FilterOperator"], function (CoreService, Sorter, Filter, FilterOperator) {                                                              
  "use strict";                                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
  var ScenarioService = CoreService.extend("be.elia.bc.CentralEntryPointApp.service.ScenarioService", {                                                                                                                                                        
    constructor: function constructor(model) {                                                                                                                                                                                                                 
      this.scenarioTitles = {};                                                                                                                                                                                                                                
      CoreService.call(this, model);                                                                                                                                                                                                                           
    },                                                                                                                                                                                                                                                         
    getTitle: function getTitle(scenarioid) {                                                                                                                                                                                                                  
      var _this = this;                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
      if (this.scenarioTitles[scenarioid]) {                                                                                                                                                                                                                   
        return Promise.resolve(this.scenarioTitles[scenarioid]);                                                                                                                                                                                               
      }                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
      return this.odata("/zcl_scenario_c('" + scenarioid + "')").get().then(function (response) {                                                                                                                                                              
        _this.scenarioTitles[scenarioid] = response;                                                                                                                                                                                                           
        return response;                                                                                                                                                                                                                                       
      });                                                                                                                                                                                                                                                      
    }                                                                                                                                                                                                                                                          
  });                                                                                                                                                                                                                                                          
  return ScenarioService;                                                                                                                                                                                                                                      
});                                                                                                                                                                                                                                                            