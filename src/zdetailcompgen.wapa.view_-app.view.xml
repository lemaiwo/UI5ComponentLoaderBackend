<mvc:View controllerName="be.wl.detailcomponentgeneric.controller.App"                                                                                                                                                                                         
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true"                                                                                                                                                                                                               
	height="100%"                                                                                                                                                                                                                                                 
	xmlns:m="sap.m"                                                                                                                                                                                                                                               
	xmlns:code="sap.ui.codeeditor"                                                                                                                                                                                                                                
	xmlns="sap.f">                                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
	<DynamicPage>                                                                                                                                                                                                                                                 
		<title>                                                                                                                                                                                                                                                      
			<DynamicPageTitle>                                                                                                                                                                                                                                          
				<heading>                                                                                                                                                                                                                                                  
					<m:FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">                                                                                                                                                                                           
						<m:Title text="Generic detail page for demo purpose" wrapping="true" class="sapUiTinyMarginEnd"/>                                                                                                                                                        
					</m:FlexBox>                                                                                                                                                                                                                                              
				</heading>                                                                                                                                                                                                                                                 
			</DynamicPageTitle>                                                                                                                                                                                                                                         
		</title>                                                                                                                                                                                                                                                     
		<content>                                                                                                                                                                                                                                                    
			<code:CodeEditor type="json" value="{/data}" width="100%" height="100%"></code:CodeEditor>                                                                                                                                                                  
		</content>                                                                                                                                                                                                                                                   
	</DynamicPage>                                                                                                                                                                                                                                                
</mvc:View>                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               